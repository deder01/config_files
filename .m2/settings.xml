<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <mirrors>
    <mirror>
      <id>sqrrl-nexus-public</id>
        <name>Sqrrl Nexus</name>
        <url>http://nexus.sqrrl-lab.net/content/groups/public</url>
        <mirrorOf>*</mirrorOf>
    </mirror>
  </mirrors>

  <servers>
    <server>
      <id>sqrrl-nexus-public</id>
      <configuration>
        <httpConfiguration>
          <all>
            <connectionTimeout>4000</connectionTimeout>
          </all>
        </httpConfiguration>
      </configuration>
    </server>
